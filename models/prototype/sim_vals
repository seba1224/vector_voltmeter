
%vamos a hacer esta prueba con un periodo de dato de 50 ciclos, ie
% pos_id =40, irig_1=25, irig_0=10...

time = [1:1:200]';

hrd_rst = zeros(200, 2);
hrd_rst(2, 2) = 1;
hrd_rst(:, 1) = time;

cal = zeros(200, 2);
cal(6, 2) = 1;
cal(:, 1) = time;

gpio = zeros(200, 2);
gpio(:, 1) = time;
gpio(10:50 ,2) = 1;         %1er pos_ident
gpio(60:100, 2) = 1;        %2do pos_ident
gpio(110:135, 2) = 1;       %irig_1
gpio(160:170, 2) = 1;       %irig_0

